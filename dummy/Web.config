<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <clear />
                <rule name="HTTP to HTTPS redirect" stopProcessing="true">
                        <match url="dummy/(.*)" />
                    <conditions>
                    <add input="{HTTPS}" pattern="off" ignoreCase="true" />
                    </conditions>
                <action type="Redirect" redirectType="Found" url="https://{HTTP_HOST}/dummy/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>